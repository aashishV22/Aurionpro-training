	public boolean areFirstAndLastTwoCharactersTheSame(String str) {
		int length=str.length();
		if(str.charAt(0)==str.charAt(1) && str.charAt(length-1)==str.charAt(length-2)) {
			return true;
		}else {
			return false;
		}
	}
}
